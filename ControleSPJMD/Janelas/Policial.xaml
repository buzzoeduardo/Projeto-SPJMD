                    <DataGridTextColumn Binding="{Binding re}" Header="RE" FontSize="14" Width="80" FontFamily="Verdana" FontWeight="Bold"/>
                    <DataGridTextColumn Binding="{Binding dig}" Header="D" FontSize="11" Width="50" FontFamily="Verdana" FontWeight="Bold"/>
