            <Button x:Name="btnOficiais" Content="OFICIAIS" Margin="10,86,10,431" Grid.Row="1" Background="#FF6374A6" BorderBrush="{x:Null}" Foreground="Black"  FontFamily="Verdana" TabIndex="2" Cursor="Hand" FontWeight="Bold" FontSize="12"/>
