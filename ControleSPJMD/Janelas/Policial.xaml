                <DropShadowEffect BlurRadius="12" Color="Black" RenderingBias="Quality" ShadowDepth="1" Direction="350"/>
            </Grid.Effect>
        </Grid>
        <Grid Grid.ColumnSpan="2" Background="#FF020873" >
